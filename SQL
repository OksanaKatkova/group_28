SELECT title, author, price, amount
FROM book
WHERE (price < 500 OR price > 600) AND amount*price >= 5000

SELECT title, author
FROM book
WHERE (price BETWEEN 540.50 AND 800)
AND amount IN (2, 3, 5, 7);

SELECT title, author FROM book
WHERE title LIKE "% %" AND title NOT LIKE " " AND author LIKE "%ะก.%"
ORDER by title ASC 

SELECT title, author FROM book
WHERE title LIKE "% %" AND title LIKE '_% _%' AND (author LIKE "% ะก._." OR author LIKE "% _.ะก.") 
ORDER by title ASC 

